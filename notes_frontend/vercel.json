{
    "version": 2,
    "builds": [
      {
        "src": "./package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://fullstack-notes-app-backend.vercel.app"
      },
      {
        "src": "/(.*)",
        "dest": "build/$1",
        "continue": true
      }
    ]
  }
  